{
  "name": "Awesome Resent Bookmarks",
  "version": "0.1.3",
  "description": "Shows recent bookmark",
  "manifest_version": 2,
  "default_locale": "en",
  "browser_action": {
    "default_title": "Awesome Resent Bookmarks",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },

  "permissions": [
    "bookmarks"
  ]
}
